{
    "name": "stocksinfo_website",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^1.6.7",
        "concurrently": "^8.2.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-static": "^1.2.6",
        "form-data": "^4.0.0",
        "http-proxy-middleware": "^3.0.0",
        "nodemon": "^3.0.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "^5.0.1",
        "web-vitals": "^3.5.2"
    },
    "scripts": {
        "start:backend": "nodemon ./backend/server.js",
        "dev": "react-scripts start",
        "build": "react-scripts build",
        "lint:js": "npx eslint src --ext .js,.jsx --fix",
        "lint:css": "npx stylelint \"src/**/*.css\" --fix -c",
        "lint:std": "npx standard --fix",
        "linter": "npm run lint:js && npm run lint:css && npm run lint:std",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "settings": {
            "react": {
                "version": "detect"
            }
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
